{
    "name": "zorachka/mapper",
    "description": "Data Mapper",
    "keywords": [
        "zorachka",
        "mapper"
    ],
    "homepage": "https://github.com/zorachka/mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\Mapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\Mapper\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
